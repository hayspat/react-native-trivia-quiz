{
  "expo": {
    "name": "A Trivia Game",
    "slug": "react-trivia-mobile",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.hayspat.trivia",
      "versionCode": 1
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
